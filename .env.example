# =============================================================================
# Spotify Data Pipeline - Environment Variables Template
# =============================================================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control
# =============================================================================

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=spotify
POSTGRES_PASSWORD=spotify_password
POSTGRES_DB=spotify_warehouse

# Kaggle API Credentials
# Get yours at: https://kaggle.com/account -> Create New Token
KAGGLE_API_TOKEN=your_kaggle_api_token

# Airflow (optional - defaults work for local dev)
AIRFLOW_UID=50000
AIRFLOW__CORE__EXECUTOR=LocalExecutor

# Grafana (optional)
GF_SECURITY_ADMIN_PASSWORD=admin

# Slack Webhook for Data Quality Alerts (optional)
# Create an incoming webhook at: https://api.slack.com/messaging/webhooks
# Format: T00000000/B00000000/XXXXXXXXXXXXXXXXXXXXXXXX
SLACK_WEBHOOK_URL=
